{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/store/slices/coursesSlice.ts",
    "utilComponents/InputField/InputField.jsx",
    "src/components/AdminSections/CohortForm/CohotForm.jsx",
    "src/components/AdminSections/CohortForm/UploadMe.tsx"
, "src/components/AdminSections/CourseNav", "src/app/admin/course/[id]/container/courseContentDetails.tsx"  ],
  "exclude": ["node_modules"]
}
